<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".MyCommdityActivity">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/padding"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/tv_return"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:clickable="true"
            android:gravity="center_vertical"
            android:text="@string/goback"
            android:textColor="@color/white"
            android:textSize="17sp"
            app:drawableStartCompat="@drawable/ic_baseline_navigate_before_24" />

        <TextView
            android:id="@+id/tv_shuaxin"
            android:clickable="true"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="end|center_vertical"
            android:text="@string/shuaxin"
            android:textSize="17sp"
            android:paddingEnd="10dp"
            android:layout_alignParentEnd="true"
            android:textColor="@color/white"/>
    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/ll_myCommodity"
            android:layout_width="wrap_content"
            android:orientation="vertical"
            android:layout_height="match_parent"/>

    </ScrollView>

</LinearLayout>